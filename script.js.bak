document.getElementById("home").addEventListener("click", function () {
  var cardx = document.getElementById("cardy");
  if (cardx.style.visibility == "hidden") {
    cardx.style.visibility = "hidden";
  } else {
    cardx.style.visibility = "visible";
  }
});

document.getElementById("ChangPwd").addEventListener("click", function () {
  var pwd = document.getElementById("formPwd");
  if (pwd.style.display == "none") {
    cardx.style.display = "none";
    pwd.style.display == "block";
  } else {
    cardx.style.display = "none";
  }
});
